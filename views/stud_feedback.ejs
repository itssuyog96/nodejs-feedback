<span id="counter">0</span>

<script>
    list = [{
        "name" : "Aditya",
        "key" : "sldhf8hweofnonncls"
    },
    {
        "name" : "Omkar",
        "key" : "sildnch8923oiecneo"
    },
    {
        "name" : "Rohil",
        "key" : "owehfc89h2389hf0df"
    }]

    addRow = function(){

        count = parseInt($('#counter').text());

        str = '<tr>'+
            '<td>'+
                '<div class="form-group col-md-10">'+
                    '<select name="student'+count+'" class="select-student select-control form-control"><option>Select</option>';

        list.forEach(student => {
            
                        str += '<option value="'+student.key+'">'+student.name+'</option>'
                
        });
        
                    str += '</select>'+
                '</div>'+
            '</td>'+
            '<td>'+
                '<div class="form-group col-md-8">'+
                    '<textarea name="comment'+count+'" col="5" row="3" class="form-control">'+
                    '</textarea>'+
                '</div>'
        '</tr>';

        $('.stud-feed-table tbody').append(str);

        $('#counter').text((count + 1).toString());
    }

    
    submit = function(){
        console.log($('#stud-feedback-form').serialize());
    }


</script>

