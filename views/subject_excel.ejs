<script>

    (load_departments = function(){
        $.ajax({
            url: '../../ajax/getDeptList',
            data: {col_id: '<%= user.col_id %>'},
            method: 'POST',
            success: function(data){
                data.forEach(function(d){
                    $('#dept_id').append('<option value="'+d.dept_id+'">'+d.dept_name+'</option>')
                });
            }
        });
    })();

    (load_surveys = function(){
        $.ajax({
            url: '../../ajax/getSurveyList',
            data: {col_id: '<%= user.col_id %>'},
            method: 'POST',
            success: function(data){
                data.forEach(function(d){
                    $('#survey_id').append('<option value="'+d.survey_id+'">'+d.survey_id+'</option>')
                });
            }
        });
    })();

   /* (load_sem = function () {
        $.ajax({
            url: '../../ajax/getSemList',
            data: {col_id: '<%= user.col_id%>'},
            method: 'POST',
            success: function (data) {
                data.forEach(function (s) {
                    $('#sem').append('<option value="'+s.sem+'">'+s.sem+'</option>')
                })
            }
        })
    })*/

    $('.portlet-body').on('click', '.tiles-subject', function(){
        $.ajax({
            url: '../../../ajax/get_sub_reports_excel',
            method: 'POST',
            data: {col_id: 1, dept_id: '1001', sem: 6, survey_id: 'survey-2017-1-even'},
            success: function(data){
                console.log(data);
            },
            error: function(error){
                console.log(error.statusText);
            }
        })
    })

    $('.download-div').on('click', '.download-excel', function(){
        window.location.replace('../../downloads/' + $('#survey_id option:selected').val() + '_' + $('#dept_id option:selected').val() + '.xlsx')
    })

    $('.portlet-body').on('click', '.tiles-department', function(){
        $('#customize').fadeIn();
        $('#customize1').fadeOut();
    })

    $('.portlet-body').on('click', '.tiles-sem', function () {
        $('#customize1').fadeIn();
        $('#customize').fadeOut();
    })

    $('.selectSurvey').on('click', '.submitFormBtn', function (e) {
        $('#error-div').fadeOut();
        $('#download_div').fadeOut();
        $('#loader').fadeIn();
        $.ajax({
            url: '../../ajax/get_dept_report_excel',
            method: 'POST',
            data: {col_id: '<%= user.col_id %>', dept_id: $('#dept_id option:selected').val(), survey_id: $('#survey_id option:selected').val()},
            success: function(data){
                $('#download_div').fadeIn();
                $('#loader').fadeOut();
            },
            error: function(error){
                $('#error-div').fadeIn();
                console.log(JSON.stringify(error));
                $('#loader').fadeOut();
            }
        })
    })
    $('.selectSurvey').on('click', '.submitFormBtn1', function (e) {
        $('#error-div').fadeOut();
        $('#download_div').fadeOut();
        $('#loader').fadeIn();
        $.ajax({
            url: '../../ajax/get_dept_report_excel',
            method: 'POST',
            data: {col_id: '<%= user.col_id %>', survey_id: $('#survey_id option:selected').val(), dept_id: $('#dept_id option:selected').val()},
            success: function (data) {
                $('#download_div').fadeIn();
                $('#loader').fadeOut();
            },
            error: function (error) {
                $('#error-div').fadeIn();
                console.log(JSON.stringify(error));
                $('#loader').fadeOut();
            }
        })
    })

</script>

