                                                        <input type="hidden" name="dept_id" value = "<%= user.dep_id %>" />
                                                        <input type="hidden" name="sem" value = "<%= user.sem %>" />
                                                        <input type="hidden" name="survey_id" value="<%= user.survey_id %>" />

                                                        <input type="hidden" name="sub_qty" value="<%= ObjectLength(subject) %>" />

