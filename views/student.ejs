                                                                        <thead>
                                                                        <% subject.forEach(function(item){%>
                                                                        <th><%= item.sub_name %></th>

                                                                        <%}); %>
                                                                        </thead>
                                                                        <tbody>
                                                                        <% particulars.faculty.questions.forEach(function(ques){ %>
